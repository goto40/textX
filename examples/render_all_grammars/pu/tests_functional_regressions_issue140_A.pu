@startuml
set namespaceSeparator .


class A.Model  {
}


class A.E  {
  ID name
}


class A.C <<abstract>> {
}


class A.C1  {
  ID name
}


class A.TXT  {
  STRING txt
}


class ID <<match>> {
}


class STRING <<match>> {
}


class BOOL <<match>> {
}


class INT <<match>> {
}


class FLOAT <<match>> {
}


class STRICTFLOAT <<match>> {
}


class NUMBER <<match>> {
}


class BASETYPE <<match>> {
}


